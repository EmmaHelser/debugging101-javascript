{
  "extends": [
    "eslint:recommended", 
    "plugin:import/errors",
    "prettier"
  ],
  "rules": { // 0 => Off 1=> Warn 2 => Warn
    "no-console": 1
  },
  "plugins": ["import"],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx":true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}